{
  "name": "libx",
  "version": "0.2.2",
  "description": "Collection + Model library for MobX",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "rimraf lib && tsc -p src",
    "test": "rimraf .test-out && tsc --skipLibCheck -p test && mocha .test-out/test/**/*.spec.js --require .test-out/test/index.js",
    "test:watch": "nodemon -e js,ts --exec npm run test",
    "lint": "tslint lib/ test/",
    "lint:watch": "nodemon --exec npm run lint",
    "cover": "nyc npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "do-publish": "npm run build && npm run lint && npm run cover && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffijoe/libx.git"
  },
  "files": [
    "lib",
    "LICENSE.md",
    "README.md"
  ],
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "mobx",
    "backbone",
    "tfrp",
    "state management",
    "react"
  ],
  "author": "Jeff Hansen <jeff@jeffijoe.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffijoe/libx/issues"
  },
  "homepage": "https://github.com/jeffijoe/libx#readme",
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.24",
    "@types/sinon-chai": "^2.7.28",
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "lodash": "^4.17.4",
    "mobx": "^3.2.2",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.13.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  },
  "peerDependencies": {
    "lodash": "^4.17.4",
    "mobx": "^3.1.9"
  },
  "dependencies": {}
}
